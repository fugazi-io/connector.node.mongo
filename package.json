  "dependencies": {
    "@fugazi/connector": "^1.0.15",
    "@types/commander": "^2.3.31",
    "@types/mongodb": "^2.1.40",
    "commander": "^2.9.0",
    "mongodb": "^2.2.24",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.1"
